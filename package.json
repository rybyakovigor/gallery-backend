{
  "name": "gallery-backend",
  "license": "UNLICENSED",
  "description": "",
  "version": "0.0.1",
  "author": "",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "check:all": "concurrently \"npm:check:*(!all)\"",
    "check:all:pre-commit": "concurrently -r \"npx lint-staged --relative\" \"npm:check:types\"",
    "check:lint": "eslint src",
    "check:prettier": "prettier --check src",
    "check:types": "tsc --noEmit",
    "fix:all": "concurrently \"npm:fix:*(!all)\"",
    "fix:lint": "eslint src --fix",
    "fix:prettier": "prettier --write src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "npx prisma generate && npx prisma format",
    "migration:generate": "npx prisma migrate dev --name $1",
    "migration:run": "npx prisma migrate deploy",
    "migration:reset": "npx prisma migrate reset",
    "prepare": "husky install"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    }
  },
  "dependencies": {
    "@nestjs/axios": "3.1.2",
    "@nestjs/cli": "10.4.7",
    "@nestjs/common": "10.4.7",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.7",
    "@nestjs/platform-express": "10.4.7",
    "@nestjs/terminus": "10.2.3",
    "@prisma/client": "5.22.0",
    "@types/multer": "1.4.12",
    "aws-sdk": "2.1692.0",
    "axios": "1.7.7",
    "class-validator": "0.14.1",
    "helmet": "8.0.0",
    "nestjs-pino": "4.1.0",
    "nestjs-zod": "4.2.0",
    "pino-http": "10.3.0",
    "pino-pretty": "12.1.0",
    "prisma": "5.22.0",
    "reflect-metadata": "0.2.2",
    "request-ip": "3.3.0",
    "rxjs": "7.8.1",
    "sharp": "0.33.5",
    "ua-parser-js": "1.0.39",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.7",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.9.0",
    "@types/request-ip": "0.0.41",
    "@types/supertest": "6.0.2",
    "@types/ua-parser-js": "0.7.39",
    "@typescript-eslint/eslint-plugin": "8.13.0",
    "@typescript-eslint/parser": "8.13.0",
    "concurrently": "9.1.0",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sonarjs": "2.0.4",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3"
  }
}
